<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE html PUBLIC "-//W3C//DTD XHTML 1.1//EN"
	"http://www.w3.org/TR/xhtml11/DTD/xhtml11.dtd">

<html xmlns="http://www.w3.org/1999/xhtml" xml:lang="en">
<head>
	<title>jQuery Calendar Date Select Plugin Example Application</title>
  <!-- Stylesheets -->
    <!-- Blue Print CSS Framework (Not Necessary, just for looks)-->
    <link rel="stylesheet" href="/stylesheets/blueprint/screen.css" type="text/css" media="screen, projection">
  	<link rel="stylesheet" href="/stylesheets/blueprint/print.css" type="text/css" media="print">
  	<!--[if IE]>
  	  <link rel="stylesheet" href="/stylesheets/blueprint/ie.css" type="text/css" media="screen, projection">
  	<![endif]-->
  	
  	<!-- Application CSS -->
  	<%= stylesheet_link_tag 'application' %>
  	
  	<!-- jQuery Calendar Date Select CSS (Copied from plugin README) -->
	  <%= stylesheet_link_tag '/stylesheets/calendar_date_select/jquery.calendardateselect.css' %>
	
	<!-- Javascript -->

	  <!-- jQuery Calendar Date Select (Copied from plugin README) -->
	  <%= javascript_include_tag 'calendar_date_select/jquery/jquery-1.3.2.min.js' %>
    <%= javascript_include_tag 'calendar_date_select/jquery/jquery.string.1.0.js' %>
    <%= javascript_include_tag 'calendar_date_select/jquery/jquery.calendardateselect.js' %>
    
	  <!-- Rails Application Javascript w/ jQuery Calendar Date Select Function Load Up -->
	  <%= javascript_include_tag 'application.js' %>
	  
</head>
<body>
  <%= @content_for_layout %>
</body>
</html>